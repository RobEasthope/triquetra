{
  "name": "triquetra",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "cd ./apps/next && yarn dev",
    "next": "cd ./apps/next && yarn dev",
    "build": "cd ./apps/next && yarn build",
